@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

 

<ol class="breadcrumb">
    <li class="breadcrumb-item">Vehicles</li>
     
</ol>


<h3>Vehicles</h3>
<div class="row">
    <div class="col-md-5">
        <ul class="list-group">
            @foreach (var vehicle in Model.Vehicles)
            {
		<hr />
		<h4>Vehicle: @vehicle.Id</h4>
                <li class="list-group-item">ID: @vehicle.Id</li>
		<li class="list-group-item">Name: @vehicle.Name</li>
		<li class="list-group-item">Abbrevation: @vehicle.Abbrevation</li>
            }
        </ul>
    </div>
</div>
